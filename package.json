{
	"name": "n8n-nodes-terraform-cloud",
	"version": "1.0.0",
	"description": "Terraform Cloud nodes for n8n (workspaces, runs, state, wait)",
	"license": "MIT",
	"homepage": "https://github.com/valkiriaaquatica/n8n-terraform-cloud",
	"keywords": [
		"n8n-community-node-package",
		"terraform",
		"terraform cloud",
		"tfe",
		"hashicorp"
	],
	"author": {
		"name": "Fernando Mendieta Ovejero"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/valkiriaaquatica/n8n-terraform-cloud.git"
	},
	"scripts": {
		"build": "n8n-node build",
		"build:watch": "tsc --watch",
		"dev": "n8n-node dev",
		"lint": "n8n-node lint",
		"lint:fix": "n8n-node lint --fix",
		"release": "n8n-node release",
		"prepublishOnly": "npm run build"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"strict": true,
		"credentials": [
			"dist/credentials/TerraformCloudApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/TerraformCloud/TerraformCloud.node.js"
		]
	},
	"devDependencies": {
		"@n8n/node-cli": "*",
		"eslint": "9.32.0",
		"prettier": "3.6.2",
		"release-it": "^19.0.4",
		"typescript": "5.9.2"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	}
}
